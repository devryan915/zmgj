					<template v-for='s in pageData.events'>
						<li class="mui-table-view-cell">
							<!--<div class="mui-slider-right mui-disabled mui-hidden">
								<a class="mui-btn mui-btn-red">删除</a>
							</div>
							<div class="mui-slider-handle mui-table">
								<div class="mui-table-cell">-->
							<div class="{{editable&&'item-cell'}}">
								<div class="item-top">
									<i class="y y-dot-circle y-3x"></i>
									<div class="item-top-content {{s.SDate|f-status s.EDate}}">
										{{s.SDate|f-time 'yyyy年MM月dd日'}}
										<span class="y-tag y-green" v-if='s.PType==1'>积</span>
										<p class="mui-pull-right y-orange">总奖金：{{s.TotalFee}}</p>
									</div>
								</div>
								<div class="y-content">
									<p>{{s.Name}}</p>
									<div>
										<div class="mui-table">
											<div class="mui-table-cell mui-col-xs-4" v-on:tap="detail(s.PID,s.RewardSourceType!=3?1:2,s.PType==1,s.Name)" v-text="s.RewardSourceType!=3?'参数名单':'支持奖励'"></div>
											<div class="mui-table-cell mui-col-xs-5" v-on:tap="detail(s.PID,s.RewardSourceType==1&&s.RewardSourceType==2?3:4,s.PType==1,s.Name)">
												<!--<span class="s-ma" v-if="s.RewardSourceType==2">马</span>-->
												<span class="s-qi" v-if="s.RewardSourceType==1">骑</span>
												<!--<i class="y y-ma" style="color:black; background-color:yellow;"></i>
												<i class="y y-qi" style="color: yellow; background-color: black;"></i>-->
												{{(s.RewardSourceType==1||s.RewardSourceType==2)?'领取奖励':'公布成绩'}}
											</div>
											<div class="mui-table-cell mui-col-xs-4" v-on:tap="detail(s.PID,5,s.PType==1)">规则</div>
										</div>
									</div>
								</div>
							</div>
							<div class="club-radio selclub-content-radio" v-if="editable">
								<!--v-model="pageData.PID"  v-on:change="del(s.eventID,$event)"-->
								<input name="radioclub" type="radio" v-on:change="del(s.RewardSourceType!=0,$event)" value="{{s.PID}}">
							</div>
							<!--</div>

							</div>-->
						</li>
					</template>
				</ul>
			</div>
